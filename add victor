#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
	int n,i;
	char a[200],b[200];

	while(scanf("%d",&n)!=EOF){
		for(i=0;i<n;i++){
			scanf("%d",&a[i]);
		}
		for(i=0;i<n;i++){
		    scanf("%d",&b[i]);
		}
		for(i=0;i<n;i++){
		    printf("%d",a[i]+b[i]);
		    printf(" ");
		}
		printf("\n");
	}
	return 0;
}
